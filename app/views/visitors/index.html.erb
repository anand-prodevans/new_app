
    <div style="margin-left:100px">
      <h4>Project Name: <%=@project.project_name%></h4>
    </div>
    <main role="main" class="container">
        <div class="panel-group" id="accordion">
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" class="accordin-image" data-parent="#accordion" href="#collapse1">Openshift Detail</a>
        </h4>
      </div>
      <div id="collapse1" class="panel-collapse collapse in">
        <div class="panel-body">
          <ul style="list-style-type:square">
            <li><strong>Lanid: <%=@user.try(:lanid)%></strong></li>
            <li><strong>URL: </strong><span>https://ose.cpaas.service.test:8443</span></li>
            <li><strong>Oc Login Setps</strong>
                <ul class="inside">
                <li>oc login https://ose.cpaas.service.test:8443</li>
                <li>Username: <%=@user.try(:lanid)%></li>
                <li>Password: (Your globaltest_account_password)</li>
                </ul>
            </li>
            <li><strong>App Url:</strong></li>
            <li><strong>Db Connect:</strong></li>
          </ul> 
        </div>
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a role="button" class="accordin-image" data-toggle="collapse" data-parent="#accordion" href="#collapse2" aria-expanded="true" aria-controls="collapseOne" class="">
          Source Control  
          </a>
        </h4>
      </div>
      <div id="collapse2" class="panel-collapse collapse">
        <div class="panel-body"><ul style="list-style-type:square">
            <li><strong>Git URL:</strong></li>
            <li><strong>Commands:</strong>
            <ul class="inside">
                <li>git clone repository url</li>
                <li>git add .</li>
                <li>git commit -m "message"</li>
                <li>git push origin</li>
            </ul>
          </li>
          </ul>
          <div class="col-md5"><button type="submit" class="btn btn-primary">Start Build</button></div>
          </div>
      </div>
    </div>
    <div class="panel panel-default">
      <div class="panel-heading">
        <h4 class="panel-title">
          <a data-toggle="collapse" class="accordin-image" data-parent="#accordion" href="#collapse3">Project Detail</a>
        </h4>
      </div>
      <div id="collapse3" class="panel-collapse collapse">
        <div class="panel-body">
          <ul style="list-style-type:none">
            <li><strong>Project startdate:</strong></li>
            <li><strong>Project enddate:</strong></li>
            <li><strong>Days Remaing:</strong></li>
            <li><strong>Extend Project date:</strong></li>
            <div class="input-group date" style="width:20%">
               <input type="text" class="form-control input-sm"><span class="input-group-addon"><i class="glyphicon glyphicon-th"></i></span>
            </div>
          </ul> 
        </div>
      </div>
    </div>
  </div> 
      <div class="col-md5"><button type="submit" class="btn btn-danger">Delete Project</button></div>
    </main>

